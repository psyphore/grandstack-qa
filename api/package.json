{
  "name": "grand-stack-starter-api",
  "version": "0.0.1",
  "description": "API app for GRANDstack",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:dev": "./node_modules/.bin/nodemon -w ./src/ --ext js,graphql --exec babel-node  ./src/index.js",
    "start:debug": "babel-node --nolazy --inspect-brk=9229 ./src/index.js",
    "build": "babel src --out-dir build && shx cp .env build 2>/dev/null || : && shx cp src/qa-schema.graphql build",
    "now-build": "babel src --out-dir build && shx cp src/qa-schema.graphql build",
    "start": "npm run build && node build/index.js",
    "seedDb": "./node_modules/.bin/babel-node src/seed/seed-db.js",
    "dockerize": "docker build --quiet --network host --pull --compress -t psyphore/graphql-api:1.0.0 ."
  },
  "author": "Sipho Hlophe",
  "license": "MIT",
  "dependencies": {
    "apollo-boost": "^0.4.9",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-http": "^1.5.17",
    "apollo-server": "^2.15.1",
    "apollo-server-lambda": "^2.15.1",
    "csv-parse": "^4.10.1",
    "dotenv": "^8.2.0",
    "graphql-tag": "^2.10.3",
    "neo4j-driver": "^4.1.0",
    "neo4j-graphql-js": "^2.14.3",
    "node-fetch": "^3.1.1",
    "neode": "^0.4.5",
    "uuid": "^8.2.0",
    "passport": "^0.4.1",
    "passport-auth0": "^1.3.3",
    "graphql-import": "^1.0.2",
    "dayjs": "^1.8.29",
    "express-jwt": "^6.0.0",
    "cors": "^2.8.5",
    "body-parser": "^1.19.0",
    "rxjs": "^6.6.0",
    "rxjs-compat": "^6.6.0",
    "graphql": "^15.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/runtime-corejs3": "^7.9.2",
    "babel-plugin-auto-import": "^1.0.5",
    "babel-plugin-module-resolver": "^4.0.0",
    "nodemon": "^2.0.4",
    "shx": "^0.3.2"
  },
  "resolutions": {
    "graphql": "15.2.0",
    "neo4j-driver": "4.1.0"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "docs/*",
      "node_modules/*",
      "meals/* ",
      "conf/*",
      "assets/*"
    ],
    "delay": "2500"
  }
}
